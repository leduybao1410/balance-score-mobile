{
  "name": "code",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "clean": "cd android && ./gradlew clean && cd .. ",
    "android": "expo run:android",
    "android --clean": "clean && android",
    "android:build": "cd android && ./gradlew assembleRelease && cd ..",
    "android:bundle": "cd android && ./gradlew bundleRelease && cd ..",
    "ios": "./build-ios.sh",
    "ios:direct": "export LANG=en_US.UTF-8 && expo run:ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "android:install": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "prepare": "husky",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/drawer": "^7.5.8",
    "@react-navigation/native": "^7.1.17",
    "@reduxjs/toolkit": "^2.9.2",
    "buffer": "^6.0.3",
    "date-fns": "^4.1.0",
    "expo": "53.0.22",
    "expo-asset": "^12.0.12",
    "expo-blur": "~14.1.5",
    "expo-clipboard": "^8.0.7",
    "expo-constants": "^17.1.7",
    "expo-crypto": "^15.0.8",
    "expo-file-system": "^19.0.21",
    "expo-font": "^14.0.10",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "^7.1.7",
    "expo-localization": "^16.1.6",
    "expo-router": "~5.1.5",
    "expo-screen-orientation": "~8.1.7",
    "expo-sharing": "^14.0.8",
    "expo-status-bar": "~2.2.3",
    "i18next": "^25.4.1",
    "lodash": "^4.17.21",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-hook-form": "^7.63.0",
    "react-i18next": "^15.7.2",
    "react-native": "0.79.5",
    "react-native-async-storage": "^0.0.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-google-mobile-ads": "^16.0.1",
    "react-native-image-picker": "^8.2.1",
    "react-native-keyevent": "^0.3.2",
    "react-native-orientation-locker": "^1.7.0",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.15.1",
    "react-native-tts": "^4.1.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-webview": "13.13.5",
    "react-native-worklets": "^0.7.1",
    "toastify-react-native": "^7.2.3",
    "xlsx": "^0.18.5",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@react-native-community/cli": "^20.0.1",
    "@types/react": "~19.0.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.25.1",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "private": true
}
